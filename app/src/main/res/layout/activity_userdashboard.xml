<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".userdashboard"
    android:id="@+id/drawer1">



    <androidx.cardview.widget.CardView
        android:layout_width="483dp"
        android:layout_height="649dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="406dp"
            android:layout_height="648dp"
            android:background="#D8D8D8">

            <Button
                android:id="@+id/button8"
                android:layout_width="167dp"
                android:layout_height="144dp"

                android:layout_marginBottom="28dp"
                android:background="#262639"
                android:clickable="true"
                android:enabled="false"
                android:text="Rent New"

                android:textAllCaps="false"
                android:textSize="20sp"
                android:textStyle="bold"
                app:backgroundTint="#262639"
                app:icon="@drawable/add1"
                app:iconGravity="textTop"
                app:iconPadding="0dp"
                app:iconSize="100dp"
                app:iconTint="#FFFFFF"
                app:iconTintMode="multiply"
                app:layout_constraintBottom_toTopOf="@+id/button9"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button9"
                android:layout_width="267dp"
                android:layout_height="105dp"
                android:layout_marginBottom="152dp"
                android:clickable="true"
                android:text="Rent History"
                app:backgroundTint="#262639"
                app:icon="@drawable/time"
                app:iconGravity="textEnd"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.474"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="263dp"
                android:layout_height="98dp"
                android:layout_marginBottom="24dp"
                android:background="#A2A2A2"
                android:text="Active Rent"
                android:textColor="#090808"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.508"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/editTextTextPersonName3"
                android:layout_width="263dp"
                android:layout_height="37dp"
                android:background="#D8D8D8"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Name                 Time"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.509"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.915" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="552dp"
                android:layout_height="258dp"
                tools:layout_editor_absoluteX="-73dp"
                tools:layout_editor_absoluteY="-102dp"
                tools:srcCompat="@tools:sample/backgrounds/scenic" />


        </androidx.constraintlayout.widget.ConstraintLayout>
        
    </androidx.cardview.widget.CardView>
    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:menu="@menu/my_menu1">


    </com.google.android.material.navigation.NavigationView>

</androidx.drawerlayout.widget.DrawerLayout>